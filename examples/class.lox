class Edible {
    init() {
        this.uses = 10;
    }
}


class Waffle < Edible {
    init() {
        this.uses = 12;
    }

    eat(condiment) {
        print "I eat my " + this.uses + "th Waffle with " + condiment;
        var uses = this.uses;
        this.uses = uses - 1;
        print "I have " + this.uses + " left.";
    }
}

var w = Waffle();
w.eat("honey");
w.eat("strawberry");
w.eat("chocolate");